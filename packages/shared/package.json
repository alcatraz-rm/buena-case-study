{
  "name": "@buena/shared",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./property": {
      "types": "./dist/property.d.ts",
      "default": "./dist/property.js"
    },
    "./building": {
      "types": "./dist/building.d.ts",
      "default": "./dist/building.js"
    },
    "./unit": {
      "types": "./dist/unit.d.ts",
      "default": "./dist/unit.js"
    },
    "./geocode": {
      "types": "./dist/geocode.d.ts",
      "default": "./dist/geocode.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "postinstall": "pnpm build"
  },
  "dependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "globals": "^16.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
